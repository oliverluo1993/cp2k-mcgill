&GLOBAL
  PROJECT LOC-CAYLEY
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  &DFT
    &PRINT
      &MO_CUBES     ! Controls the printing of the MOs in the output and in the *.cube files
      NHOMO 5       ! Number of HOMOs to be printed (count starts from the highest occupied orbital. -1 = all). Here 5.
      NLUMO 5       ! Number of LUMOs to be printed (count starts from the lowest unoccupied orbital). Here 5. 
      &END MO_CUBES
    &END PRINT
    BASIS_SET_FILE_NAME BASIS_SET
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 200
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-11
    &END QS
    &SCF
      EPS_SCF 1.0E-4
      SCF_GUESS ATOMIC
    &END SCF
    &XC
      &XC_FUNCTIONAL Pade
      &END XC_FUNCTIONAL
    &END XC
    &LOCALIZE
      METHOD CAYLEY
      &CAYLEY_OPTIMIZER
          MAX_ITER                    1000
          EPS_ERROR                   1.0E-6
          LIN_SEARCH_EPS_ERROR        1.0E-3
          LIN_SEARCH_STEP_SIZE_GUESS  0.0001
          MAX_ITER_OUTER_LOOP         0
          PRECONDITIONER              NONE
          CONJUGATOR                  POLAK_RIBIERE
      &END CAYLEY_OPTIMIZER
      &PRINT
        &WANNIER_CUBES
        &END
      &END
      !STATES ALL
      !&PRINT 
      !  &PROGRAM_RUN_INFO LOW
      !  &END PROGRAM_RUN_INFO
      !&END PRINT
      !&PRINT
      !  &WANNIER_CENTERS
      !    IONS+CENTERS
      !    FORMAT XMOL
      !  &END
      !  &MOLECULAR_DIPOLES
      !    COMMON_ITERATION_LEVELS 2
      !    PERIODIC
      !  &END
      !  &TOTAL_DIPOLE
      !    COMMON_ITERATION_LEVELS 2
      !    PERIODIC T
      !    REFERENCE COAC
      !  &END
      !&END
    &END
  &END DFT
  &SUBSYS
    &CELL
      ABC 10.0 10.0 10.0
    &END CELL
    &COORD
       C      -1.855000   -0.242000   -0.433000
       C      -0.587000    0.489000   -0.003000
       C       0.592000   -0.480000   -0.025000
       C       1.855000    0.245000    0.429000
       H      -2.697000    0.450000   -0.417000
       H      -2.050000   -1.067000    0.254000
       H      -1.725000   -0.633000   -1.442000
       H      -0.717000    0.880000    1.006000
       H      -0.392000    1.313000   -0.689000
       H       0.733000   -0.857000   -1.038000
       H       0.389000   -1.313000    0.648000
       H       1.714000    0.622000    1.442000
       H       2.057000    1.079000   -0.244000
       H       2.697000   -0.447000    0.413000
    &END COORD
    &KIND C
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-BLYP-q4
    &END KIND
    &KIND H
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q1
    &END KIND
    &KIND O
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
